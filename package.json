{
  "scripts": {
    "lint:tslint": "vue-tsc -p tsconfig.json --noEmit",
    "lint:eslint": "eslint . --no-eslintrc --config .eslintrc.js --ext .js,.ts,.vue,.mts",
    "lint:stylelint": "stylelint **/*.{css,scss,vue} --allow-empty-input",
    "lint:fix": "npm run lint:eslint -- --fix && npm run lint:stylelint -- --fix && npm run lint:tslint",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:tslint",
    "dev": "vitepress dev src",
    "build": "vitepress build src",
    "start": "vitepress preview src"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^11.1.0",
    "element-plus": "^2.8.4",
    "vee-validate": "^4.13.2",
    "vitepress": "^1.3.4",
    "vue": "^3.5.10"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "1.7.0",
    "@types/node": "20.16.10",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.19.2",
    "postcss": "8.4.33",
    "postcss-html": "1.6.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.1.1",
    "sass": "^1.79.4",
    "stylelint": "16.9.0",
    "stylelint-config-css-modules": "4.4.0",
    "stylelint-config-recess-order": "5.1.0",
    "stylelint-config-recommended": "14.0.1",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard-scss": "13.1.0",
    "typescript": "5.3.3",
    "vue-eslint-parser": "9.4.0",
    "vue-tsc": "^2.1.6"
  },
  "prettier": "./prettier.config.js"
}
